<UserControl x:Class="JsonLibraryImportExport.View.JsonLibraryImportExportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JsonLibraryImportExport.View"
             mc:Ignorable="d">
    <DockPanel Margin="20">
        <StackPanel DockPanel.Dock="Bottom" Margin="0,15,0,0">
            <Button Content="Import All Games" Command="{Binding ImportAllGamesCommand}" />
            <Button Content="Export All Games" Command="{Binding ExportAllGamesCommand}" />
        </StackPanel>
    </DockPanel>
</UserControl>
